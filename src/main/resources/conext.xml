<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


    <bean class="myspring.InjectRandomIntAnnotationBeanPostProcessor"/>

    <bean id="quote" class="java.lang.String">
        <constructor-arg value="get daaaaaaaaaaawm"/>
    </bean>
    <bean id="shakespearQuoter" class="quoters.ShakespearQuoter">
        <property name="message" value="2 B || ! 2 B"/>
    </bean>

    <bean id="terminatorQuoter" class="quoters.TerminatorQuoter" destroy-method="killAll">
        <property name="messages">
            <list>
                <value>I'll be back</value>
                <ref bean="quote"/>
                <value>Astalavista baby</value>
            </list>
        </property>
    </bean>
    <bean id="robot" class="quoters.TalkingRobotImpl">
        <property name="quoters">
            <list>
                <ref bean="terminatorQuoter"/>
                <ref bean="shakespearQuoter"/>
            </list>
        </property>
    </bean>
    <bean class="org.springframework.context.annotation.CommonAnnotationBeanPostProcessor"/>
    <bean class="myspring.BenchmarkProxyConfigurer"/>

    <bean class="quoters.BeanDefinitionAnalyzerBeanFactoryPostProcessor"/>
</beans>